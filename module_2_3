my_list = [42, 69, 0, 322, 13, 0, 99, -5, 9, 8, 7, -6, 5]

k = 0  # переменая счётчик
print(my_list)
while k < len(my_list):   # контроль длинны списка
	if my_list[k] == 0:  # если в списке 0, удаляе 
		del my_list[k]
		continue             # после удаления, перезапуск
	elif my_list[k] < 0:  # отрецательное число, СТОП
		break
	else:
		k += 1    # контроль длинны списка без нуля

print(my_list[:k:1]) # вывод списка по счётчикучётчику